{
  "name": "neotools",
  "version": "0.0.1",
  "description": "Composable command line tools and reference implementations for Neo Smart Economy function primitives",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetter/neotools.git"
  },
  "scripts": {
    "build": "",
    "lint": "eslint ./",
    "lintFix": "eslint ./ --fix",
    "start": "",
    "test": "jest"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "enzyme": "3.1.1",
    "enzyme-to-json": "3.2.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^0.30.0",
    "html-loader": "^0.5.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "jest-cli": "^22.4.4",
    "prettier": "^0.11.0"
  },
  "dependencies": {
    "neon-js": "^1.1.2",
    "bignumber.js": "5.0.0",
    "bluebird": "^3.3.4",
    "bs58": "^4.0.1",
    "classnames": "^2.1.3",
    "commander": "^2.17.1",
    "eslint-cli": "^1.1.1",
    "history": "^4.7.2",
    "underscore": "^1.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|gif|eot|otf|svg|png|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "__tests__/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx}",
      "src/js/**/*.{js,jsx}",
      "src/js/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setupTests.js",
    "testURL": "http://localhost"
  },
  "author": "https://github.com/phetter",
  "license": "MIT"
}
